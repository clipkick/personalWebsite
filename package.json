    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "history": "^5.0.0",
    "mongoose": "^5.12.11",
    "multer": "^1.4.2",
    "node-html-parser": "^3.3.3",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^6.6.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router": "^6.0.0-beta.0",
